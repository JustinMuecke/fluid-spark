sbt "runMain Main resources/configs/DOGS_exp/CC/2013/CC-2013-3.conf"
sbt "runMain Main resources/configs/DOGS_exp/CC/2013/CC-2013-4.conf"
sbt "runMain Main resources/configs/DOGS_exp/CC/2013/CC-2013-5.conf"
sbt "runMain Main resources/configs/DOGS_exp/CC/2013/CC-2013-6.conf"
sbt "runMain Main resources/configs/DOGS_exp/CC/2013/CC-2013-7.conf"
sbt "runMain Main resources/configs/DOGS_exp/CC/2013/CC-2013-8.conf"
sbt "runMain Main resources/configs/DOGS_exp/CC/2013/CC-2013-9.conf"

